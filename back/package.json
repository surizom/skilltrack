  "scripts": {
    "build": "tsc",
    "check": "npm run lint && tsc --noEmit",
    "dev": "npm i && npm run start",
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "lint:fix": "npm run lint -- --fix",
    "lint:report": "npm run lint -- -f junit --quiet -o ./reports/junit-eslint.xml",
    "integration": "docker-compose -f docker-compose.test.yml build && docker-compose -f docker-compose.test.yml run --rm integration-back-test npm run integration:docker",
    "integration:docker": "npm run test --  --runInBand --config jest.integration.config.js",
    "start": "nodemon",
    "test": "jest --passWithNoTests",
    "test:ci": "npm run test && npm run lint:report",
    "test:debug": "node --inspect-brk=9228 node_modules/jest/bin/jest.js --runInBand"
  }
